#include<stdio.h>
#include<string.h>
void main()
{
 int i,a,b,c,d,e;
 char p[]="2+2*3+2/2-1";
 a=p[0]-'0';
 for(i=1;i<strlen(p);i++)
 {
  if(p[i]=='*')
 {
  b=p[i+1]-'0';
  a=a*b;
  i++;
  if(p[i]=='+'){
  d=p[i+1]-'0';
  a=a+d;
  i++;}
  else if(p[i]=='-')
  {
   e=p[i+1]-'0';
   a=a+e;
   i++;
  }
 }
 else if(p[i]=='/')
 {
  c=p[i+1]-'0';
  a=a/c;
  i++;}
  if(p[i]=='+'){
  d=p[i+1]-'0';
  a=a+d;
  i++;}
  else if(p[i]=='-')
  {
   e=p[i+1]-'0';
   a=a+e;
   i++;
   }
  }
  printf("a=%d",a);
 }
